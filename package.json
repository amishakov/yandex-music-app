{
  "name": "yandex-music-app",
  "version": "1.8.0",
  "author": "juvirez",
  "description": "just webview for yandex music with native features",
  "homepage": "https://yandex-music.juvs.dev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juvirez/yandex-music-app.git"
  },
  "main": "src/main/index.js",
  "scripts": {
    "prestart": "vite build",
    "start": "electron .",
    "prebuild": "vite build",
    "build": "electron-builder",
    "publish": "electron-builder -p always",
    "postinstall": "electron-rebuild",
    "watch": "vite build --watch"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.2.10",
    "@vitejs/plugin-vue": "^4.2.1",
    "electron": "^19.0.3",
    "electron-builder": "^23.0.4",
    "electron-notarize": "^1.1.1",
    "node-gyp": "^9.3.1",
    "vite": "^4.3.3",
    "vite-plugin-electron-renderer": "^0.14.1"
  },
  "dependencies": {
    "discord-rpc": "^4.0.1",
    "electron-google-analytics": "^1.0.0",
    "electron-media-service": "juvirez/electron-media-service",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.1",
    "node-addon-api": "^3.0.0",
    "node-fetch": "^2.6.7",
    "vue": "^3.2.47"
  },
  "build": {
    "productName": "Yandex Music Unofficial",
    "appId": "dev.juvs.yandex-music",
    "afterSign": "./notarize.js",
    "mac": {
      "category": "public.app-category.music",
      "target": {
        "target": "default",
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "icon": "static/icon.icns",
      "publish": "github"
    },
    "extraResources": [
      "static/tray*.png",
      "static/touchbar/*.png",
      "static/default_cover_*.png"
    ]
  }
}
